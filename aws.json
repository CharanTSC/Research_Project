{
  "provider": {
    "aws": {
      "region": "us-east-1"
    }
  },
  "resource": {
    "aws_instance": {
      "compute_with_storage": {
        "ami": "ami-12345678",
        "instance_type": "t3.micro",
        "associate_public_ip_address": true,
        "root_block_device": {
          "volume_size": 40,
          "volume_type": "io2"
        }
      }
    }
  }
}
